
<Page xmlns="http://schemas.nativescript.org/tns.xsd" xmlns:maps="nativescript-google-maps-sdk" navigatingTo="onNavigatingTo" class="page">
    
    <Page.actionBar>
        <ActionBar title="State of Campus" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>

<GridLayout>
	<maps:mapView
    latitude="{{ latitude }}"
    longitude="{{ longitude }}"
    zoom="{{ zoom }}"
    mapReady="{{ onMapReady }}"
	/>
	<Button text="Lists" class="listButton" tap="moveToList" horizontalAlignment="left"/>
  </GridLayout>
</Page>
